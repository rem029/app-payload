{
  "name": "do-app-backend-admin",
  "description": "Rem Apps Admin Panel",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts & nodemon tailwind:watch",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "yarn copyfiles && yarn tailwind:build && yarn build:payload && yarn build:server",
    "start": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "start:pm2": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production pm2 start dist/server.js --name=do-app-backend-admin",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "tailwind:watch": "tailwindcss -i ./tailwind.css -o ./src/css/compiledTailwind.css --watch",
    "tailwind:build": "tailwindcss -i ./tailwind.css -o ./src/css/compiledTailwind.css",
    "migrate:create": "payload migrate:create",
    "migrate:latest": "payload migrate",
    "migrate:revert": "payload migrate:down"
  },
